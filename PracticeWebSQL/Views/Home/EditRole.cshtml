@model PracticeWebSQL.Models.Role
@{
    ViewBag.Title = "Переименовывание роли";
}

<form asp-action="EditRole" asp-controller="home">
    <input type="hidden" asp-for="ID"/>
    <div class="form-group">
        <input type="text" asp-for="Name" placeholder="Email" class="form-control"/>
    </div>
    <div class="form-group">
        <input type="submit" value="Сохранить изменения" class="btn btn-default"/>
    </div>
</form>
